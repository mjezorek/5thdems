   <% include ../../app/header %>
   <div id="page-wrapper" >
    <div id="page-inner">
      <div class="row">
        <div class="col-md-12">
         <h2>Meetings</h2>   
       </div>
     </div>         
     <% if (message.length > 0) { %>
        <div class="alert alert-danger"><%= message %></div>
    <% } %>     
     <!-- /. ROW  -->
     <form id="attendance_form" method="post" action="/membership/meetings/attendance/<%=meetings.id%>">
     <hr />
     <% console.log(meetings)%>
     <div class="row">
      <div class="col-lg-12 ">
       <b>Meeting:</b> <%= meetings.name %><br />
       <b>Date:</b> <%= meetings.date %><br />
       <b>Type:</b> <%= meetings.type %><br />
       <b>Location:</b> <%= meetings.location %><br />
      </div>
    </div>
    <hr />
    <div class="form-group">
     <div class="col-sm-12">
       <label>Members</label>
       <select multiple class="form-control" id="member_select_list" name="attendance">
       <% for(i in members) { %>
          <option value="<%=members[i].id%>"
          <% if(meetings.attendance && meetings.attendance.indexOf(members[i].id) != "-1") {%>
          selected
          <%}%>
          ><%=members[i].first_name%> <%=members[i].last_name %></option>
         <%}%>
       </select>
     </div>
    </div>
    <div class="form-group">
     <div class="col-sm-12">
       <label>Additional Members or Comments</label>
       <textarea name="comments" class="form-control" ><%= meetings.comments %></textarea>
     </div>
    </div>
    <br /><br /> <br /><br /> <br /><br />
    <button type="submit" class="btn btn-warning btn-lg">Update Attendance</button>
    </form>
    <!-- /. ROW  --> 


</div>
<!-- /. PAGE INNER  -->
</div>
<% include ../../app/footer %>
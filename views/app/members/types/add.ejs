 <% include ../../../app/header %>
 <div id="page-wrapper" >
          <div id="page-inner">

    <h1><span class="fa fa-sign-in"></span>New Membership Type</h1>
    <% if (message.length > 0) { %>
        <div class="alert alert-danger"><%= message %></div>
    <% } %>

    <form action="/membership/members/types/add" method="post">
        <div class="form-group">
            <label>Name</label>
            <input type="text" class="form-control" name="name">
        </div>
        <div class="form-group">
            <label>Cost</label>
            $<input type="text" class="form-control" name="cost">
        </div>
        <button type="submit" class="btn btn-warning btn-lg">Add</button>
    </form>

    <a href="/membership/members/types">Return to List</a>
    </div>
    </div>
 <% include ../../../app/footer %>